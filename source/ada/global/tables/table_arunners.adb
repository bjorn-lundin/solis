    if JSON_Data.Has_Field("status") then
      Move( Source => JSON_Data.Get("status"), Target => Data.Status , Drop => Right);
    end if;

    if JSON_Data.Has_Field("handicap") then
      declare
        Tmp : Float := 0.0;
      begin
        Tmp := JSON_Data.Get("handicap");
        Data.Handicap := Fixed_Type(Tmp);
      end;
    end if;

    if JSON_Data.Has_Field("runnername") then
      Move( Source => JSON_Data.Get("runnername"), Target => Data.Runnername , Drop => Right);
    end if;

    if JSON_Data.Has_Field("runnernamestripped") then
      Move( Source => JSON_Data.Get("runnernamestripped"), Target => Data.Runnernamestripped , Drop => Right);
    end if;

    if JSON_Data.Has_Field("runnernamenum") then
      Move( Source => JSON_Data.Get("runnernamenum"), Target => Data.Runnernamenum , Drop => Right);
    end if;

    if JSON_Data.Has_Field("ixxlupd") then
      Move( Source => JSON_Data.Get("ixxlupd"), Target => Data.Ixxlupd , Drop => Right);
    end if;

    if JSON_Data.Has_Field("ixxluts") then
      declare
        Tmp : String := JSON_Data.Get("ixxluts");
