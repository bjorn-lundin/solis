    if The_Tag = Table_Okmarkets_Set_Name then
      Handler.Found_Set := False;
    elsif The_Tag = Table_Okmarkets_Row_Name then
      if Handler.Found_Set then
        Handler.Okmarkets_List.Append(Handler.Okmarkets_Data);
        Handler.Okmarkets_Data := Empty_Data;
      end if;
    end if;
  exception
    when Ada.Strings.Length_Error => Handler.OK := False;
  end End_Element;
