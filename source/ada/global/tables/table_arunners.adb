  overriding procedure Characters(Handler          : in out Arunners_Reader;
                       Ch               : Unicode.CES.Byte_Sequence := "") is
    function To_Iso_Latin_15(Str : Unicode.CES.Byte_Sequence) return String is
      use Unicode.Encodings;
    begin
      return  Convert(Str, Get_By_Name("utf-8"), Get_By_Name("iso-8859-15"));
    end To_Iso_Latin_15;
    The_Tag   : constant String := To_String(Handler.Current_Tag);
    The_Value : constant String := To_Iso_Latin_15(Ch);
    procedure Fix_String (Value    : String;
                          Variable : in out String) is
    begin
      Append(Handler.Accumulated, Value);
      Ada.Strings.Fixed.Move(To_String(Handler.Accumulated), Variable);
    end Fix_String;
  begin
    if Handler.Found_Set then
      if The_Tag = Marketid_Name then
       Fix_String(The_Value, Handler.Arunners_Data.Marketid);
      elsif The_Tag = Selectionid_Name then
       Handler.Arunners_Data.Selectionid := Integer_4'value(The_Value);
      elsif The_Tag = Sortprio_Name then
       Handler.Arunners_Data.Sortprio := Integer_4'value(The_Value);
      elsif The_Tag = Status_Name then
       Fix_String(The_Value, Handler.Arunners_Data.Status);
      elsif The_Tag = Handicap_Name then
       Handler.Arunners_Data.Handicap := Fixed_Type'value(The_Value);
      elsif The_Tag = Runnername_Name then
       Fix_String(The_Value, Handler.Arunners_Data.Runnername);
      elsif The_Tag = Runnernamestripped_Name then
       Fix_String(The_Value, Handler.Arunners_Data.Runnernamestripped);
      elsif The_Tag = Runnernamenum_Name then
       Fix_String(The_Value, Handler.Arunners_Data.Runnernamenum);
      elsif The_Tag = Ixxlupd_Name then
       Fix_String(The_Value, Handler.Arunners_Data.Ixxlupd);
      elsif The_Tag = Ixxluts_Name then
       Handler.Arunners_Data.Ixxluts := Calendar2.To_Time_Type(The_Value(1..11), The_Value(13..24));
      end if;
    end if;
  exception
    when Ada.Strings.Length_Error => Handler.OK := False;
  end Characters;

