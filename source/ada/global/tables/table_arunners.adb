  overriding procedure End_Element(Handler       : in out Arunners_Reader;
                        Namespace_URI : Unicode.CES.Byte_Sequence := "";
                        Local_Name    : Unicode.CES.Byte_Sequence := "";
                        Qname         : Unicode.CES.Byte_Sequence := "") is
    pragma Warnings(Off,Namespace_URI);
    pragma Warnings(Off,Qname);
    The_Tag : constant String := Local_Name;
  begin
    if The_Tag = Table_Arunners_Set_Name then
      Handler.Found_Set := False;
    elsif The_Tag = Table_Arunners_Row_Name then
      if Handler.Found_Set then
        Handler.Arunners_List.Append(Handler.Arunners_Data);
        Handler.Arunners_Data := Empty_Data;
      end if;
    end if;
  exception
    when Ada.Strings.Length_Error => Handler.OK := False;
  end End_Element;
