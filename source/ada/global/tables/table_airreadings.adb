  procedure From_Xml(Xml_Filename : in Unbounded_String;
                     A_List       : in out Airreadings_List_Pack2.List) is
    My_Reader   : Airreadings_Reader;
    Input       : File_Input;
  begin
    My_Reader.Airreadings_List := A_List;
