    if JSON_Data.Has_Field("backprice") then
      declare
        Tmp : Float := 0.0;
      begin
        Tmp := JSON_Data.Get("backprice");
        Data.Backprice := Fixed_Type(Tmp);
      end;
    end if;

    if JSON_Data.Has_Field("layprice") then
      declare
        Tmp : Float := 0.0;
      begin
        Tmp := JSON_Data.Get("layprice");
        Data.Layprice := Fixed_Type(Tmp);
      end;
    end if;

    if JSON_Data.Has_Field("ixxlupd") then
      Move( Source => JSON_Data.Get("ixxlupd"), Target => Data.Ixxlupd , Drop => Right);
    end if;

    if JSON_Data.Has_Field("ixxluts") then
      declare
        Tmp : String := JSON_Data.Get("ixxluts");
      begin  -- "marketStartTime":"2013-06-22T17:39:00.000Z"
        Data.Ixxluts := Calendar2.To_Time_Type(Tmp(1..10), Tmp(12..23));
      end;
    end if;

    return Data;
  end From_JSON;

  function To_Xml(Data      : in Table_Aprices.Data_Type;
                  Ret_Start : in Boolean;
                  Ret_Data  : in Boolean;
                  Ret_End   : in Boolean) return String is
    Ls      : constant String := "";
    S_Start : constant String := "<APRICES_ROW>" & Ls;
    S_End   : constant String := "</APRICES_ROW>" & Ls;
    S1 : constant String :=
          "<MARKETID>" & Utils.Trim(Data.Marketid) & "</MARKETID>" & Ls;
    S2 : constant String :=
          "<SELECTIONID>" & Utils.Trim(Data.Selectionid'Img) & "</SELECTIONID>" & Ls;
    S3 : constant String :=
          "<PRICETS>" & Calendar2.String_Date_And_Time(Data.Pricets, Milliseconds => True) & "</PRICETS>" & Ls;
    S4 : constant String :=
          "<STATUS>" & Utils.Trim(Data.Status) & "</STATUS>" & Ls;
    S5 : constant String :=
          "<TOTALMATCHED>" & Utils.F8_Image(Data.Totalmatched) & "</TOTALMATCHED>" & Ls;
    S6 : constant String :=
          "<BACKPRICE>" & Utils.F8_Image(Data.Backprice) & "</BACKPRICE>" & Ls;
    S7 : constant String :=
          "<LAYPRICE>" & Utils.F8_Image(Data.Layprice) & "</LAYPRICE>" & Ls;
    S8 : constant String :=
          "<IXXLUPD>" & Utils.Trim(Data.Ixxlupd) & "</IXXLUPD>" & Ls;
    S9 : constant String :=
          "<IXXLUTS>" & Calendar2.String_Date_And_Time(Data.Ixxluts, Milliseconds => True) & "</IXXLUTS>" & Ls;
