<?xml version="1.0" encoding="ISO-8859-15"?> 
<BnlBot xmlns="http://www.nonodev.com/bnlbot" xmlns:xcl="http://www.nonodev.com/bnlbot" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Table Desc="logged readings airquality" Name="airreadings" Tablespace="" >
        <Column Name="macaddress"    AllowNull="0" Description="mac id"/>
        <Column Name="created"       AllowNull="0" Description="timestamp"/>
        <Column Name="temperature"   AllowNull="0" Description="raw temp value"/>
        <Column Name="pressure"      AllowNull="0" Description="raw pressure value"/>
        <Column Name="humidity"      AllowNull="0" Description="raw humidity value"/>
        <Column Name="gasresistance" AllowNull="0" Description="raw gasresistance value"/>
        <Column Name="altitude"      AllowNull="1" Description="raw altitude value"/>
        <Column Name="ixxlupd"       AllowNull="0" Description="Latest updater"/>
        <Column Name="ixxluts"       AllowNull="0" Description="Latest update timestamp"/>
        <Index Columns="macaddress,created" type="primary"/>
    </Table> 
</BnlBot>
